import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Insira o valor para o cálculo:");
		int n = sc.nextInt();
				
		for (int i = n; i > 0; i--) {
				//n = n * (i - 1);
				System.out.println(i);
			}
		//System.out.println("O fatorial é: " + n);
				
		sc.close();	
	}

}