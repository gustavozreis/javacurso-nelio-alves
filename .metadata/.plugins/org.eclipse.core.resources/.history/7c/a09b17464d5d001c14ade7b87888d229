import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Insira o valor para o cálculo:");
		int n = sc.nextInt();
				
		for (int i = n; i >= 1; i--) {
				int n = n * i;
				System.out.println(n);
			}
		//System.out.println("O fatorial é: " + n);
				
		sc.close();	
	}

}