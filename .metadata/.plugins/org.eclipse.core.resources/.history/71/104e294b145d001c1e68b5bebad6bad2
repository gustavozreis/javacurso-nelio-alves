import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		String senha = "";
		System.out.println("Insira a senha:");
		
		while (senha != "2002") {
			senha = sc.next();
			System.out.println("Senha invalida");
		}
		
		System.out.println("Acesso Permitido");
		
		sc.close();
						
	}
}