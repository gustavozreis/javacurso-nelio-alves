import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Insira o código e a quantidade do item:");
				
		int itemNum;
		int itemQuant;
		double totalPedido;
		
		itemNum = sc.nextInt();
		itemQuant = sc.nextInt();
		
		if (itemNum == 1) {
			totalPedido = 4.00 * itemQuant;
		} else if (itemNum == 2) {
			totalPedido = 4.50 * itemQuant;
		} else if (itemNum == 3) {
			totalPedido = 5.00 * itemQuant;
		} else if (itemNum == 4) { 
			totalPedido = 2.00 * itemQuant;
		} else {
			totalPedido = 1.50 * itemQuant;
		}
		
		sc.close();
		
	}
}