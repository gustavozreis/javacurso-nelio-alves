import java.util.Scanner;
import java.util.Locale;

public class Main {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Insira o valor para o cálculo:");
		int n = sc.nextInt();
		
		if (n == 0) {
			System.out.println("O fatorial é: 0");
		} else {
		for (int i = n; i > 0; i--) {
			n = n * (n - i - 1);
			}
		System.out.println("O fatorial é: " + n);
		}
		

		
	}
}